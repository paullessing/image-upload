{
  "name": "image-upload",
  "version": "0.0.1",
  "description": "Image upload service in Node and Express",
  "main": "dist/index.js",
  "repository": "git@github.com:paullessing/image-upload",
  "author": "paullessing <paul@paullessing.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "node node_modules/typescript/bin/tsc",
    "build-dev": "node node_modules/typescript/bin/tsc --watch --outDir tmp",
    "nodemon": "node node_modules/nodemon/bin/nodemon tmp/index.js",
    "serve": "node node_modules/concurrently/src/main --kill-others --prefix \"[{name}]\" --names \"tsc ,node\" --prefix-colors \"blue,magenta\" \"yarn build-dev\" \"yarn nodemon\""
  },
  "dependencies": {
    "busboy": "^0.2.14",
    "express": "^4.15.2",
    "express-router-decorators": "paullessing/express-router-decorators",
    "file-type": "^4.2.0",
    "inversify": "^4.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "reflect-metadata": "^0.1.10",
    "stream-meter": "^1.0.4",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/express": "^4.0.35",
    "@types/file-type": "^0.0.2",
    "@types/mkdirp": "^0.3.29",
    "@types/morgan": "^1.7.32",
    "@types/stream-meter": "^0.0.19",
    "@types/uuid": "^2.0.29",
    "@types/winston": "^2.3.1",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.3.1"
  }
}
