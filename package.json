{
  "name": "image-upload",
  "version": "0.0.1",
  "description": "Image upload service in Node and Express",
  "main": "dist/index.js",
  "repository": "git@github.com:paullessing/image-upload",
  "author": "paullessing <paul@paullessing.com>",
  "license": "MIT",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "build": "tsc",
    "build-dev": "tsc --watch --outDir tmp",
    "nodemon": "nodemon tmp/index.js",
    "serve": "concurrently --kill-others --prefix \"[{name}]\" --names \"tsc ,node\" --prefix-colors \"blue,magenta\" \"yarn build-dev\" \"yarn nodemon\""
  },
  "dependencies": {
    "aws-sdk": "^2.62.0",
    "busboy": "^0.2.14",
    "express": "^4.15.2",
    "express-router-decorators": "paullessing/express-router-decorators",
    "file-type": "^7.6.0",
    "inversify": "^4.1.0",
    "mime-types": "^2.1.15",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "mongodb": "^3.0.5",
    "morgan": "^1.8.1",
    "reflect-metadata": "^0.1.10",
    "s3-upload-stream": "^1.0.7",
    "sharp": "^0.20.1",
    "stream-meter": "^1.0.4",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/express": "^4.0.35",
    "@types/file-type": "^5.2.1",
    "@types/mime-types": "^2.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mongodb": "^3.0.9",
    "@types/morgan": "^1.7.32",
    "@types/s3-upload-stream": "^1.0.0",
    "@types/sharp": "^0.17.1",
    "@types/stream-meter": "^0.0.21",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.3",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.8.1"
  }
}
